<Window x:Class="MDZFBLACommunityService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDZFBLACommunityService"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="MainWindow" Height="675" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image Source="/resources/LogowName.png" Height="200" Margin="62,10,74,436" x:Name="Logo" Width="258" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Grid HorizontalAlignment="Left" Height="358" Margin="45,230,0,0" VerticalAlignment="Top" Width="301">
            <TextBox HorizontalAlignment="Left" Height="30" Margin="75,0,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="194" BorderBrush="Black"/>
            <Label Content="Username" HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top" Foreground="#DD000000"/>
            <Label Content="Password/ID&#xA;" HorizontalAlignment="Left" Margin="0,35,0,0" VerticalAlignment="Top" Foreground="#DD000000" Height="30" Width="76"/>
            <PasswordBox HorizontalAlignment="Left" Margin="75,35,0,0" VerticalAlignment="Top" Width="194" BorderBrush="Black"/>
            <Button x:Name="True" Content="Button" HorizontalAlignment="Left" Margin="81,265,0,0" VerticalAlignment="Top" Width="140" Height="53" Click="True_Click"/>
        </Grid>
    </Grid>
</Window>
